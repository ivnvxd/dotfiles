Include ~/.ssh/config_*

# Default configuration for your main Git account
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519

# SSH key for the Obsidian Git account
Host obsidian-git
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519_obsidian
    IdentitiesOnly yes
